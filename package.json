{
  "name": "ember-cli-chart",
  "version": "3.7.2",
  "description": "Ember component for ChartJS",
  "keywords": [
    "ember-addon"
  ],
  "homepage": "https://github.com/aomran/ember-cli-chart",
  "bugs": {
    "url": "https://github.com/aomran/ember-cli-chart/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aomran/ember-cli-chart.git"
  },
  "license": "MIT",
  "author": "Ahmed Omran",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@ember/render-modifiers": "^2.0.5",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "chart.js": "^4.2.1",
    "ember-auto-import": "^2.6.0",
    "ember-cli-babel": "^7.26.6",
    "ember-cli-htmlbars": "^6.2.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.0.1",
    "@ember/test-helpers": "^2.4.2",
    "@embroider/test-setup": "^2.1.1",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.28.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^1.0.0",
    "ember-qunit": "^5.1.4",
    "ember-resolver": "^10.0.0",
    "ember-source": "~3.28.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.5.1",
    "ember-try": "^2.0.0",
    "eslint": "^9.11.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^11.4.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^7.3.4",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "qunit": "^2.16.0",
    "qunit-dom": "^2.0.0",
    "release-it": "^15.6.0",
    "release-it-lerna-changelog": "^5.0.0",
    "webpack": "^5.75.0"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": false
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  }
}
